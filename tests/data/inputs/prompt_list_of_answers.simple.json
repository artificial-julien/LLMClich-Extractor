{
  "foreach": [
    {
      "node_type": "variables",
      "list": [
        {
          "statement": "The Earth is flat."
        },
        {
          "statement": "The Sun revolves around the Earth."
        }
      ]
    },
    {
      "node_type": "prompt_list_of_answers",
      "models": [
        {
          "name": "gpt-4o-mini",
          "temperature": 0.0,
          "iterations": 1
        }
      ],
      "prompts": [
        "Is this statement scientifically accurate: '[statement]'"
      ],
      "possible_answers": [
        "True",
        "False"
      ],
      "result_var_name": "accuracy"
    },
    {
      "node_type": "export_to_csv",
      "output_file": "output.csv",
      "columns": [
        "statement",
        "accuracy",
        "model-name",
        "temperature",
        "prob_1_True",
        "prob_2_False"
      ]
    }
  ]
} 