{
  "foreach": [
    {
      "node_type": "variables",
      "list": [
        {
          "adjective": "happy",
          "noun": "dog",
          "verb": "runs"
        },
        {
          "adjective": "lazy",
          "noun": "cat",
          "verb": "sleeps"
        },
        {
          "adjective": "clever",
          "noun": "fox",
          "verb": "jumps"
        }
      ]
    },
    {
      "node_type": "prompt_list_of_answers",
      "models": [
        {
          "name": "gpt-4o-mini",
          "temperature": 0.0,
          "iterations": 1
        }
      ],
      "prompts": [
        "Is this sentence grammatically correct: 'The [adjective] [noun] [verb] quickly'?"
      ],
      "possible_answers": [
        "Yes",
        "No"
      ],
      "result_var_name": "is_grammatical"
    },
    {
      "node_type": "export_to_csv",
      "output_file": "output.csv",
      "columns": [
        "adjective",
        "noun",
        "verb",
        "is_grammatical"
      ]
    }
  ]
} 