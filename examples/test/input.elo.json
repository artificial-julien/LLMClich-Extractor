{
  "foreach": [
    {
      "node_type": "prompt_elo_rating",
      "models": [
        {
          "name": "gpt-4.1-nano",
          "temperature": 0.5,
          "top_p": 1.0,
          "iterations": 2
        }
      ],
      "competitors": [
        "Albert Einstein",
        "Abraham Lincoln",
        "Bill Gates",
        "Charles Darwin",
        "Donald Trump",
        "Kim Kardashian",
        "Snoop Dogg",
        "George W. Bush",
        "Emmanuel Macron",
        "The average person"
      ],
      "prompts": [
        "Who of [_elo_match_competitor_a] and [_elo_match_competitor_b] is the smartest?",
        "Who of [_elo_match_competitor_a] and [_elo_match_competitor_b] is the least dumb?"
      ],
      "batches_per_model": 5,
      "initial_rating": 1000,
      "symmetric_matches": true
    },
    {
      "node_type": "export_to_csv",
      "output_file": "output_results.elo.ranking.csv",
      "columns": [
        "_seed",
        "_model_name",
        "_model_temperature",
        "_model_top_p",
        "_elo_competitor",
        "_elo_rating",
        "_elo_wins",
        "_elo_loss",
        "_elo_draws"
      ]
    },
    {
      "node_type": "export_to_csv",
      "output_file": "output_results.elo.matches.csv",
      "skip_empty_rows": false,
      "columns": [
        "_seed",
        "_model_name",
        "_model_temperature",
        "_model_top_p",
        "_elo_match_competitor_a",
        "_elo_match_competitor_b",
        "_elo_match_winner",
        "_elo_match_draw",
        "_elo_match_wins_a",
        "_elo_match_wins_b"
      ]
    }
  ]
}