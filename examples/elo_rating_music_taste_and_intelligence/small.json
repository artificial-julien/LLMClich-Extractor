    {
      "node_type": "export_to_csv",
      "output_file": "output_results.small.elo.matches.csv",
      "skip_empty_rows": true,
      "columns": [
        "_seed",
        "_model_name",
        "_model_temperature",
        "_model_top_p",
        "_elo_match_competitor_a",
        "_elo_match_competitor_b",
